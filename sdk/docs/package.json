{
    "name": "@iota/sdk-docs",
    "version": "1.0.9",
    "private": true,
    "description": "Docs for @iota npm packages",
    "main": "index.js",
    "scripts": {
        "dev": "next dev",
        "#build": "next cache causes issues in vercel: https://github.com/orgs/vercel/discussions/2079",
        "build": "typedoc && next build && rm -rf .next/cache",
        "start": "next start",
        "prettier:check": "prettier -c --ignore-unknown .",
        "prettier:fix": "prettier -w --ignore-unknown .",
        "eslint:check": "eslint --max-warnings=0 .",
        "eslint:fix": "pnpm run eslint:check --fix",
        "lint": "pnpm run eslint:check && pnpm run prettier:check",
        "lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix"
    },
    "publishConfig": {
        "access": "restricted"
    },
    "keywords": [],
    "author": "IOTA Foundation <contact@iota.org>",
    "license": "Apache-2.0",
    "dependencies": {
        "@iota/bcs": "workspace:*",
        "@iota/dapp-kit": "workspace:*",
        "@iota/deepbook": "workspace:*",
        "@iota/enoki": "workspace:*",
        "@iota/graphql-transport": "workspace:*",
        "@iota/kiosk": "workspace:*",
        "@iota/iota-sdk": "workspace:*",
        "@iota/wallet-standard": "workspace:*",
        "@iota/zklogin": "workspace:*",
        "@iota/zksend": "workspace:*",
        "@tanstack/react-query": "^5.50.1",
        "@types/node": "^20.14.10",
        "next": "^14.2.4",
        "nextra": "^2.13.4",
        "nextra-theme-docs": "^2.13.4",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@types/react": "^18.3.3",
        "typedoc": "^0.26.3"
    }
}
